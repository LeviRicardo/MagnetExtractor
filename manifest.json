{
    "manifest_version": 2,
    "name": "Magnegetter",
    "version": "1.0",
    "description": "Get all magnet links from the present page",
    "icons": {
        "48": "imgs/favicon.ico"
    },
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": {
        "48": "imgs/favicon.ico"
      }
    }
}